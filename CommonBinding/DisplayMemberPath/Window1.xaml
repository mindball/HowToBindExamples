<Window x:Class="CommonBinding.DisplayMemberPath.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommonBinding.DisplayMemberPath"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Window.Resources>
        <local:GreekGods x:Key="goose" />
    </Window.Resources>

    <ItemsControl ItemsSource="{StaticResource goose}" DisplayMemberPath="Name" />
</Window>
